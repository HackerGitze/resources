simulations=c()
for (i in 1:200){
w=mean(rnorm(5,2,3))
simulations=c(simulations,w)
}
dat=data.frame(simulations=simulations)
library(ggplot2)
ggplot(dat, aes(x=simulations))+
geom_histogram()
